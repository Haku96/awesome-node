{
  "name": "awesome-node",
  "version": "1.0.0",
  "packageManager": "pnpm@9.7.0",
  "author": "Jerry",
  "license": "MIT",
  "keywords": [
    "node"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "docs:build": "pnpm -F=vitepress build",
    "docs:dev": "pnpm -F=vitepress dev",
    "preinstall": "npx only-allow pnpm",
    "push-after-commit": "git push",
    "prezx": "pnpm --version",
    "zx": "zx ./playground/zx.mjs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.6.0",
    "eslint": "catalog:",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.3.0",
    "internal-ip": "^8.0.0",
    "lint-staged": "^15.2.11",
    "minimist": "^1.2.8",
    "pathe": "^2.0.2",
    "picocolors": "^1.1.1",
    "prettier": "catalog:",
    "prompts": "^2.4.2",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:",
    "unbuild": "^3.3.1",
    "zx": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "post-commit": "npm run push-after-commit"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,vue,md}": [
      "prettier --write"
    ]
  }
}
